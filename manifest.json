{ 
    "name": "Pickle", 
    "description": "Pickle chrome extension",
    "version": "1.0",
    "author": "Honnur V",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {
        "default_icon": "tick-gray32.png",
        "default_popup": "popup.html",
        "default_title": "Pickle"
    },
    "permissions": [
        "tabs","activeTab","storage","http://*/*"
     ],
     "manifest_version": 2
}